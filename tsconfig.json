{
    "compilerOptions":
    {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "Node",
        "outDir": "dist",
        "sourceMap": true,
        "esModuleInterop": true,
        "strict": true,
        "resolveJsonModule": true
    },
    "include": [
        "src/**/*.ts",
        "types.d.ts"
    ],
    "exclude": ["node_modules"]
}